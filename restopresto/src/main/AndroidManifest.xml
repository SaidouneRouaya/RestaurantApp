            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="example.android.com.RestoPresto.MenuDesMenusActivity" />
        </activity>
        <activity
            android:name="example.android.com.RestoPresto.MenuDesMenusActivity"
            android:label="Choix du menu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="example.android.com.RestoPresto.RestaurantActivity" />
        </activity>
        <activity android:name="example.android.com.RestoPresto.InfosActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="example.android.com.RestoPresto.RestaurantActivity" />
        </activity>
        <meta-data android:name="com.google.android.geo.API_KEY"
            android:value="@string/bundle_key"/>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>